{
  "expo": {
    "name": "Athan",
    "slug": "athan",
    "version": "1.0.12",
    "orientation": "portrait",
    "scheme": "athan",
    "userInterfaceStyle": "automatic",
    "icon": "./assets/icons/config/ios.png",
    "backgroundColor": "#2c1c77",
    "splash": {
      "image": "./assets/icons/config/splash.png",
      "backgroundColor": "#2c1c77"
    },
    "ios": {
      "icon": "./assets/icons/config/ios.png",
      "supportsTablet": true,
      "config": {
        "usesNonExemptEncryption": false
      },
      "bundleIdentifier": "com.mugtaba.athan",
      "infoPlist": {
        "CFBundleDisplayName": "Athan",
        "NSUserNotificationsUsageDescription": "We need to send you prayer time notifications"
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/icons/config/android-adaptive.png",
        "monochromeImage": "./assets/icons/config/android-adaptive.png",
        "backgroundColor": "#2c1c77"
      },
      "package": "com.mugtaba.athan"
    },
    "plugins": [
      "expo-font",
      [
        "expo-notifications",
        {
          "icon": "./assets/icons/config/ios.png",
          "defaultChannel": "default",
          "sounds": [
            "./assets/audio/athan1.wav",
            "./assets/audio/athan2.wav",
            "./assets/audio/athan3.wav",
            "./assets/audio/athan4.wav",
            "./assets/audio/athan5.wav",
            "./assets/audio/athan6.wav",
            "./assets/audio/athan7.wav",
            "./assets/audio/athan8.wav",
            "./assets/audio/athan9.wav",
            "./assets/audio/athan10.wav",
            "./assets/audio/athan11.wav",
            "./assets/audio/athan12.wav",
            "./assets/audio/athan13.wav",
            "./assets/audio/athan14.wav",
            "./assets/audio/athan15.wav"
          ]
        }
      ],
      [
        "expo-splash-screen",
        {
          "image": "./assets/icons/config/splash.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#2c1c77"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "eas": {
        "projectId": "feadd49a-8467-4860-95c5-8ae0c8bc119b"
      }
    }
  }
}
