{
  "expo": {
    "name": "athan",
    "slug": "athan",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon-512x512.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#000000"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.mugtaba.athan",
      "infoPlist": {
        "UIBackgroundModes": ["audio", "fetch", "remote-notification"]
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/icon-512x512.png",
        "backgroundColor": "#ffffff"
      }
    },
    "plugins": [
      [
        "expo-notifications",
        {
          "icon": "./assets/icon-512x512.png",
          "color": "#ffffff",
          "defaultChannel": "default",
          "sounds": [
            "./assets/athan.wav"
          ]
        }
      ],
      [
        "expo-build-properties",
        {
          "ios": {
            "newArchEnabled": true
          },
          "android": {
            "newArchEnabled": true
          }
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "eas": {
        "projectId": "feadd49a-8467-4860-95c5-8ae0c8bc119b"
      }
    }
  }
}
