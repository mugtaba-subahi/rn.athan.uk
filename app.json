{
  "expo": {
    "newArchEnabled": true,
    "name": "Athan",
    "slug": "athan",
    "version": "1.0.24",
    "orientation": "portrait",
    "scheme": "athan",
    "userInterfaceStyle": "dark",
    "icon": "./assets/icons/config/icon-ios.png",
    "backgroundColor": "#2c1c77",
    "splash": {
      "image": "./assets/icons/config/icon-splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#2c1c77"
    },
    "ios": {
      "icon": "./assets/icons/config/icon-ios.png",
      "supportsTablet": true,
      "config": {
        "usesNonExemptEncryption": false
      },
      "bundleIdentifier": "com.mugtaba.athan",
      "infoPlist": {
        "CFBundleDisplayName": "Athan",
        "NSUserNotificationsUsageDescription": "We need to send you prayer time notifications",
        "UIBackgroundModes": ["fetch"]
      },
      "backgroundColor": "#2c1c77"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/icons/config/icon-android-adaptive.png",
        "backgroundColor": "#2c1c77"
      },
      "package": "com.mugtaba.athan",
      "permissions": [
        "RECEIVE_BOOT_COMPLETED",
        "POST_NOTIFICATIONS",
        "USE_EXACT_ALARM",
        "SCHEDULE_EXACT_ALARM",
        "WAKE_LOCK"
      ]
    },
    "plugins": [
      [
        "expo-splash-screen",
        {
          "backgroundColor": "#2c1c77",
          "image": "./assets/icons/config/icon-splash.png",
          "resizeMode": "contain"
        }
      ],
      [
        "expo-font",
        {
          "fonts": ["./assets/fonts/Roboto-Regular.ttf", "./assets/fonts/Roboto-Medium.ttf"]
        }
      ],
      [
        "expo-notifications",
        {
          "icon": "./assets/icons/config/icon-android-notification.png",
          "sounds": [
            "./assets/audio/athan1.wav",
            "./assets/audio/athan2.wav",
            "./assets/audio/athan3.wav",
            "./assets/audio/athan4.wav",
            "./assets/audio/athan5.wav",
            "./assets/audio/athan6.wav",
            "./assets/audio/athan7.wav",
            "./assets/audio/athan8.wav",
            "./assets/audio/athan9.wav",
            "./assets/audio/athan10.wav",
            "./assets/audio/athan11.wav",
            "./assets/audio/athan12.wav",
            "./assets/audio/athan13.wav",
            "./assets/audio/athan14.wav",
            "./assets/audio/athan15.wav",
            "./assets/audio/athan16.wav"
          ]
        }
      ],
      [
        "react-native-permissions",
        {
          "iosPermissions": []
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "eas": {
        "projectId": "feadd49a-8467-4860-95c5-8ae0c8bc119b"
      }
    }
  }
}
