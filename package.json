{
  "name": "athan",
  "version": "1.0.15",
  "type": "commonjs",
  "private": true,
  "main": "expo-router/entry",
  "scripts": {
    "start": "expo start -c",
    "ios": "expo run:ios",
    "android": "expo run:android",
    "reset": "yarn clean && yarn install && yarn husky && yarn start",
    "clean": "rm -rf ios android node_modules yarn.lock .expo .vscode .husky && yarn cache clean && rm -rf /tmp/metro-*",
    "add": "echo 'Use: `npx expo install <package>` for Expo/RN packages, yarn add <package> for others'",
    "husky": "husky install && husky add .husky/pre-commit 'npx lint-staged'"
  },
  "lint-staged": {
    "**/*.{js,jsx,ts,tsx,mjs}": [
      "prettier --write",
      "eslint --fix"
    ]
  },
  "dependencies": {
    "@gorhom/bottom-sheet": "^5.0.6",
    "@shopify/react-native-skia": "1.2.3",
    "date-fns": "^4.1.0",
    "date-fns-tz": "^3.2.0",
    "expo": "^51.0.38",
    "expo-av": "~14.0.7",
    "expo-constants": "~16.0.2",
    "expo-dev-client": "~4.0.28",
    "expo-font": "~12.0.10",
    "expo-haptics": "~13.0.1",
    "expo-linking": "~6.3.1",
    "expo-notifications": "~0.28.19",
    "expo-router": "~3.5.24",
    "expo-splash-screen": "~0.27.7",
    "expo-status-bar": "~1.12.1",
    "expo-system-ui": "~3.0.7",
    "expo-updates": "~0.25.27",
    "jotai": "^2.10.1",
    "react": "18.2.0",
    "react-native": "0.74.5",
    "react-native-edge-to-edge": "^1.4.0",
    "react-native-gesture-handler": "~2.16.1",
    "react-native-indicators": "^0.17.0",
    "react-native-mmkv": "2.12.2",
    "react-native-pager-view": "6.3.0",
    "react-native-reanimated": "~3.10.1",
    "react-native-safe-area-context": "4.10.5",
    "react-native-screens": "3.31.1",
    "react-native-svg": "15.2.0"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@types/node": "^22.9.1",
    "@types/react": "~18.2.45",
    "@types/react-native-indicators": "0.16.6",
    "eslint": "9.15.0",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-unused-imports": "^4.1.4",
    "husky": "^8.0.3",
    "lint-staged": "^15.2.0",
    "pino": "^9.5.0",
    "pino-pretty": "^13.0.0",
    "prettier": "^3.4.1",
    "react-native-svg-transformer": "^1.5.0",
    "typescript": "~5.3.3",
    "typescript-eslint": "^8.16.0"
  },
  "peerDependencies": {
    "expo": "*"
  },
  "peerDependenciesMeta": {
    "expo": {
      "optional": true
    }
  }
}
