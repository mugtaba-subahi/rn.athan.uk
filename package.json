{
  "name": "athan",
  "version": "1.0.26",
  "type": "commonjs",
  "private": true,
  "main": "expo-router/entry",
  "scripts": {
    "start": "expo start -c",
    "ios": "expo run:ios",
    "android": "expo run:android",
    "reset": "yarn clean && yarn install && yarn husky && yarn start",
    "clean": "rm -rf ios android node_modules yarn.lock .expo .vscode .husky && yarn cache clean && rm -rf /tmp/metro-*",
    "add": "echo 'Use: `npx expo install <package>` for Expo/RN packages, yarn add <package> for others'",
    "husky": "husky install && husky add .husky/pre-commit 'npx lint-staged'"
  },
  "lint-staged": {
    "**/*.{js,jsx,ts,tsx,mjs}": [
      "prettier --write",
      "eslint --fix"
    ]
  },
  "dependencies": {
    "@gorhom/bottom-sheet": "^5.1.1",
    "date-fns": "^4.1.0",
    "date-fns-tz": "^3.2.0",
    "expo": "^52.0.36",
    "expo-av": "^15.0.2",
    "expo-background-fetch": "^13.0.5",
    "expo-constants": "^17.0.7",
    "expo-dev-client": "^5.0.12",
    "expo-font": "^13.0.4",
    "expo-haptics": "^14.0.1",
    "expo-intent-launcher": "^12.0.2",
    "expo-linear-gradient": "^14.0.2",
    "expo-linking": "^7.0.5",
    "expo-notifications": "^0.29.13",
    "expo-router": "^4.0.17",
    "expo-splash-screen": "^0.29.22",
    "expo-system-ui": "^4.0.8",
    "expo-task-manager": "^12.0.5",
    "expo-updates": "^0.26.19",
    "jotai": "^2.12.1",
    "react": "^18.3.0",
    "react-native": "~0.77.1",
    "react-native-edge-to-edge": "^1.1.6",
    "react-native-gesture-handler": "^2.23.1",
    "react-native-mmkv": "^3.2.0",
    "react-native-pager-view": "^6.7.0",
    "react-native-permissions": "^5.2.5",
    "react-native-reanimated": "4.0.0-beta.2",
    "react-native-safe-area-context": "^5.2.0",
    "react-native-screens": "^4.8.0",
    "react-native-svg": "^15.11.1"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@types/node": "^22.9.1",
    "@types/react": "^18.2.45",
    "eslint": "^9.15.0",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-unused-imports": "^4.1.4",
    "husky": "^8.0.3",
    "lint-staged": "^15.2.0",
    "pino": "^9.5.0",
    "pino-pretty": "^13.0.0",
    "prettier": "^3.4.1",
    "react-native-svg-transformer": "^1.5.0",
    "typescript": "^5.3.3",
    "typescript-eslint": "^8.16.0"
  }
}
